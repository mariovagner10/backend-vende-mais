{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["deno.ns", "dom"],
    "types": ["deno.ns"],
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
